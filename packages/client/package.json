{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "build:client": "rimraf dist/client && vite build --outDir dist/client",
    "build:server": "rimraf dist/server && vite build --outDir dist/server --ssr src/entry-server.tsx",
    "build": "yarn run build:server && yarn run build:client",
    "start": "NODE_ENV=production node server",
    "lint:js": "eslint --color --ext \".js,.jsx,.ts,.tsx\" .",
    "lint:scss": "stylelint --cache --cache-location \"node_modules/.cache/stylelint/.stylelintcache\" --ignore-path .gitignore \"{**/*,*}.{css,scss}\"",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "format:prettier": "prettier --write .",
    "format:imports": "npm run lint:js -- --fix",
    "format": "npm-run-all -l -p \"format:**\"",
    "test:watch": "npm run test:only -- --watch",
    "test:only": "jest ./",
    "test:coverage": "npm run test:only -- --coverage",
    "test": "npm run test:only"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^8.5.0",
    "js-cookie": "^3.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/jest": "^28.1.8",
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "compression": "^1.7.4",
    "eslint": "^8.23.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.1.1",
    "msw": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^4.4.1",
    "sass": "^1.58.3",
    "serve-static": "^1.15.0",
    "stylelint": "^15.2.0",
    "stylelint-config-clean-order": "^5.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-order": "^6.0.2",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.4.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.9.5",
    "vite": "^3.0.7",
    "vite-imagetools": "^4.0.18"
  },
  "license": "MIT"
}
